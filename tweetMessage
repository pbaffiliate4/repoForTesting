var t = require("twitter")
var count = 0;

if(!storage.local.counter)
    storage.local.counter = count;
else
  count = storage.local.counter;

count++;
storage.local.counter = count;

//return
return t.tweet("this is Juls' tweet #" + count + " coming from socialMedia/tweetsd");   				   				   				   				   				   				   							   				   							